pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
               git branch: 'main', credentialsId: 'krish', url: 'https://github.com/krishnarju/Testapp-1.git'
            }
        }
        stage('build') {
            steps {
               sh 'mvn -B -DskipTests clean package'
            }
        }
       stage('Scan') {
                    parallel {
                        stage("Sonar Scan") {
                           
                            steps {
                                sonarscan(sonar)
                            }
                        }
                    }
        } // End of Sonar Scan
                        
    stage("NexusIQ Scan") {
                            steps {
                                nexusIQScan(nexusIQ)
                            }
        }
} 
        } // End of Sonar Scan
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
