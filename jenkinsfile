pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
               git branch: 'main', credentialsId: 'krish', url: 'https://github.com/krishnarju/Testapp-1.git'
            }
        }
        stage('build') {
            steps {
               sh 'mvn -B -DskipTests clean package'
            }
        }
       stage('CodeCoverage') {
         steps {
             withSonarQubeEnv('sonarqube') {  bat "${scannerHome}/bin/sonar-scanner.bat"}
            script{ System.getenv().put('sonar-project.properties', 'D:/$$chidambaram$$/Action-master/sonar-project.properties')
                
            }
             bat "sonar-scanner -Dsonar.projectKey=org.konakart:Konakart -Dsonar.sources=."
         }
     }
                        
    stage("NexusIQ") {
                            steps {
                                nexusIQScan(nexusIQ)
                            }
        }
} 
         } // End of Sonar Scan
        
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
